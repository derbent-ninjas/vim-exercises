{
  "name": "dn-ve--front",
  "version": "0.0.1",
  "description": "Next js frontend application for derbent-ninjas/vim-exercises project",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start --port 3001",
    "lint": "next lint",
    "test": "jest"
  },
  "keywords": [],
  "author": "https://github.com/derbent-ninjas",
  "license": "MIT",
  "dependencies": {
    "dn-ve--errors": "workspace:*",
    "dn-ve--eslint": "workspace:*",
    "dn-ve--prettier": "workspace:*",
    "dn-ve--tsconfig": "workspace:*",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.50.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/supertest": "^2.0.10",
    "eslint": "^7.22.0",
    "eslint-config-next": "^12.1.5",
    "jest": "^26.6.3",
    "jest-extended": "^2.0.0",
    "jest-junit": "^13.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "clearMocks": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
